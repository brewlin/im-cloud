{
    "name": "brewlin/cloud",
    "type": "project",
    "keywords": [
        "center",
        "swoole",
        "grpc",
        "framework"
    ],
    "description": "Modern High performance Coroutine PHP Application, base on Swoole",
    "license": "Apache-2.0",
    "require": {
        "php": ">=7.0",
        "swoole/etcd-client": "^1.0",
        "swoft/log": "~2.0.0",
        "nikic/fast-route": "^1.3",
        "vlucas/phpdotenv": "^2.5",
        "swoft/swoole-ide-helper": "dev-master",
        "psr/http-message": "^1.0",
        "toolkit/cli-utils": "~1.0",
        "brewlin/im-grpc":"@dev",
        "brewlin/im-core":"@dev"

    },
    "autoload": {
        "psr-4":{
            "App\\":"app/"
        }
    },
    "repositories": {
        "0":{
            "type":"path",
            "url":"../../package/im-grpc"
        },
        "1":{
            "type":"path",
            "url":"../../package/im-core"
        },
        "2":{
            "type":"path",
            "url":"../../package/im-discovery"
        },
        "packagist": {
            "type": "composer",
            "url": "https://packagist.laravel-china.org"
        }
    }
}
