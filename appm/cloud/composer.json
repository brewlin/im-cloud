      "1":{
        "type":"path",
        "url":"../../pkg/core"
      },
      "2":{
        "type":"path",
        "url":"../../pkg/discovery"
      },
      "3":{
        "type":"path",
        "url":"../../pkg/process"
      },
      "4":{
        "type":"path",
        "url":"../../pkg/queue"
      },
      "5":{
        "type":"path",
        "url":"../../pkg/log"
      },
      "6":{
        "type":"path",
        "url":"../../pkg/stdlib"
      },
      "7":{
        "type":"path",
        "url":"../../pkg/redis"
      },
      "8":{
        "type":"path",
        "url":"../../pkg/task"
      },
      "9":{
        "type":"path",
        "url":"../../pkg/memory"
      },
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    }
}
