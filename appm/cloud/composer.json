{
    "name": "brewlin/cloud",
    "type": "project",
    "keywords": [
        "center",
        "swoole",
        "grpc",
        "framework"
    ],
    "description": "Modern High performance Coroutine PHP Application, base on Swoole",
    "license": "Apache-2.0",
    "require": {
        "php": ">=7.0",
        "swoole/etcd-client": "^1.0",
        "brewlin/log": "@dev",
        "brewlin/stdlib":"@dev",
        "nikic/fast-route": "^1.3",
        "vlucas/phpdotenv": "^2.5",
        "psr/http-message": "^1.0",
        "toolkit/cli-utils": "~1.0",
        "brewlin/im-grpc":"@dev",
        "brewlin/im-core":"@dev",
        "brewlin/im-discovery":"@dev",
        "brewlin/im-process":"@dev",
        "brewlin/im-queue":"@dev",
        "brewlin/im-redis":"@dev",
        "brewlin/im-task":"@dev",
        "brewlin/im-memory":"@dev"

    },
    "autoload": {
        "psr-4":{
            "App\\":"app/"
